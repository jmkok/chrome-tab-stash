{
	"manifest_version": 2,

	"name": "Tab stasher",
	"description": "This extension stashes pages for later use",
	"version": "0.0",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},
	"options_page": "options.html",
	"permissions": [
		"storage",
		"tabs",
		"activeTab"
	]
}
